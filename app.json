{
  "expo": {
    "name": "emirouq-mobile",
    "slug": "emirouq-mobile",
    "version": "1.0.0",
    "backgroundColor": "#12121C",
    "scheme": "emirouq-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo/Emi-logo.png"
    },
    "plugins": [
      ["expo-localization"],
      [
        "expo-router",
        {
          "origin": "http://example.com/"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "AsyncStorage_db_size_in_MB": "500"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/logo/Emi-logo.png",
          "dark": {
            "image": "./assets/logo/Emi-logo.png",
            "backgroundColor": "#000000"
          },
          "resizeMode": "cover"
        }
      ],
      ["expo-font"],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logo/Emi-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo/Emi-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "locales": {
      "en": "./utils/i18n/locales/en.json",
      "ar": "./utils/i18n/locales/ar.json"
    },
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.emirouq",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Emi-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emirouq",
      "permissions": ["android.permission.RECORD_AUDIO", "android.permission.RECORD_AUDIO"]
    },
    "newArchEnabled": true,
    "extra": {
      "supportsRTL": true,
      "router": {
        "origin": "http://example.com/"
      },
      "eas": {
        "projectId": "9c51ca7a-dd5c-46e1-9089-d32a655b5e7e"
      }
    },
    "owner": "emirouq",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9c51ca7a-dd5c-46e1-9089-d32a655b5e7e"
    }
  }
}
