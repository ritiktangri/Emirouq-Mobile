{
  "expo": {
    "name": "emirouq-mobile",
    "slug": "emirouq-mobile",
    "version": "1.0.0",
    "backgroundColor": "#12121C",
    "scheme": "emirouq-mobile",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo/Emi-logo.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.emirouq",
          "enableGooglePay": false,
          "publishableKey": "pk_test_51N2v0aJ3xk1X4Z5j6m7g8h9i0j0k0l0m0n0o0p0q0r0s0t0u0v0w0x0y0z0A1B2C3D4E5F6G7H8I9J"
        }
      ],
      [
        "expo-localization"
      ],
      [
        "expo-router",
        {
          "origin": "http://example.com/"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "AsyncStorage_db_size_in_MB": "500"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/logo/Emi-logo.png",
          "dark": {
            "image": "./assets/logo/Emi-logo.png",
            "backgroundColor": "#000000"
          },
          "resizeMode": "cover"
        }
      ],
      [
        "expo-font"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-web-browser",
        {
          "experimentalLauncherActivity": true
        }
      ],
      "expo-localization",
      "expo-video",
      "expo-audio",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/logo/Emi-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo/Emi-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "locales": {
      "en": "./utils/i18n/locales/en.json",
      "ar": "./utils/i18n/locales/ar.json"
    },
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ExpoLocalization_supportsRTL": true,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show your current position."
      },
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.emirouq",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Emi-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emirouq",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "WAKE_LOCK"
      ]
    },
    "newArchEnabled": true,
    "extra": {
      "supportsRTL": true,
      "router": {
        "origin": "http://example.com/"
      },
      "eas": {
        "projectId": "db3fbf5a-4e2b-48f9-bf5f-0aea1aa5bb39"
      }
    },
    "owner": "emirouq-team",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/db3fbf5a-4e2b-48f9-bf5f-0aea1aa5bb39"
    }
  }
}
